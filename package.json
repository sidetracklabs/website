{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && next-sitemap && next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext mdx,md,js,jsx,ts,tsx",
    "start": "next start",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "next": "14.1.1",
    "nextra": "2.13.3",
    "nextra-theme-docs": "2.13.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.33.2",
    "sidetrack": "^0.0.16",
    "vitest": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@mdx-js/mdx": "3.0.1",
    "@sidetrack/client-prisma": "0.0.16",
    "@types/node": "20.11.24",
    "@types/pg": "8.11.2",
    "autoprefixer": "10.4.17",
    "commitizen": "4.3.0",
    "cz-git": "1.8.0",
    "effect": "2.4.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-prettier": "5.1.3",
    "gh-pages": "6.1.1",
    "husky": "9.0.11",
    "next-sitemap": "4.2.3",
    "pg": "8.11.3",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "rehype-raw": "7.0.0",
    "remark-shiki-twoslash": "3.1.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.15.4"
}
