{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && next-sitemap && next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "next start",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "next": "15.1.2",
    "nextra": "3.3.0",
    "nextra-theme-docs": "3.3.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.33.5",
    "sidetrack": "^0.0.18",
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@mdx-js/mdx": "3.1.0",
    "@sidetrack/client-prisma": "0.0.18",
    "@types/node": "22.10.2",
    "@types/pg": "8.11.10",
    "autoprefixer": "10.4.20",
    "commitizen": "4.3.1",
    "cz-git": "1.11.0",
    "effect": "3.11.9",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-prettier": "5.2.1",
    "gh-pages": "6.2.0",
    "husky": "9.1.7",
    "next-sitemap": "4.2.3",
    "pg": "8.13.1",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2"
  },
  "packageManager": "pnpm@9.15.1"
}
