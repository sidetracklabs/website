{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && next-sitemap && next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext mdx,md,js,jsx,ts,tsx",
    "start": "next start",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "next": "14.0.4",
    "nextra": "2.13.2",
    "nextra-theme-docs": "2.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.33.1",
    "sidetrack": "^0.0.15",
    "vitest": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@mdx-js/mdx": "3.0.0",
    "@sidetrack/client-prisma": "0.0.15",
    "@types/node": "20.10.6",
    "@types/pg": "8.10.9",
    "autoprefixer": "10.4.16",
    "commitizen": "4.3.0",
    "cz-git": "1.8.0",
    "effect": "2.0.0-next.62",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-mdx": "2.3.1",
    "eslint-plugin-prettier": "5.1.2",
    "gh-pages": "6.1.1",
    "husky": "8.0.3",
    "next-sitemap": "4.2.3",
    "pg": "8.11.3",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "rehype-raw": "7.0.0",
    "remark-shiki-twoslash": "3.1.3",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.14.0"
}
