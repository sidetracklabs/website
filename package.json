{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && next-sitemap && next build && touch out/.nojekyll && pnpm search-build",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . && prettier --check .",
    "lint-fix": "eslint --fix . && prettier --write .",
    "prepare": "husky",
    "search-build": "pagefind --site .next/server/app --output-path out/_pagefind",
    "start": "next start"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "next": "15.2.0",
    "nextra": "4.2.13",
    "nextra-theme-docs": "4.2.13",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.21.0",
    "@mdx-js/mdx": "3.1.0",
    "@sidetrack/client-prisma": "0.1.7",
    "@types/node": "22.13.8",
    "@types/pg": "8.11.11",
    "commitizen": "4.3.1",
    "cz-git": "1.11.0",
    "effect": "3.13.4",
    "eslint": "9.21.0",
    "eslint-config-next": "15.2.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-mdx": "3.1.5",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "next-sitemap": "4.2.3",
    "pagefind": "1.3.0",
    "pg": "8.13.3",
    "prettier": "3.5.2",
    "prettier-plugin-sort-json": "4.1.1",
    "sidetrack": "0.1.7",
    "typescript": "5.8.2",
    "vitest": "3.0.7"
  },
  "packageManager": "pnpm@10.4.1"
}
