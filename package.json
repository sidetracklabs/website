{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && next-sitemap && next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext mdx,md,js,jsx,ts,tsx",
    "start": "next start",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "next": "14.2.2",
    "nextra": "2.13.4",
    "nextra-theme-docs": "2.13.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.33.3",
    "sidetrack": "^0.0.18",
    "vitest": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@mdx-js/mdx": "3.0.1",
    "@sidetrack/client-prisma": "0.0.18",
    "@types/node": "20.12.7",
    "@types/pg": "8.11.5",
    "autoprefixer": "10.4.19",
    "commitizen": "4.3.0",
    "cz-git": "1.9.1",
    "effect": "3.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-prettier": "5.1.3",
    "gh-pages": "6.1.1",
    "husky": "9.0.11",
    "next-sitemap": "4.2.3",
    "pg": "8.11.5",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "rehype-raw": "7.0.0",
    "remark-shiki-twoslash": "3.1.3",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@9.0.4"
}
