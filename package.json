{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && next-sitemap && next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext mdx,md,js,jsx,ts,tsx",
    "start": "next start",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "next": "13.4.19",
    "nextra": "2.10.0",
    "nextra-theme-docs": "2.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.5",
    "sidetrack": "^0.0.14",
    "vitest": "^0.34.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@effect/io": "0.40.1",
    "@mdx-js/mdx": "2.3.0",
    "@sidetrack/client-prisma": "0.0.14",
    "@types/node": "20.6.0",
    "@types/pg": "8.10.2",
    "autoprefixer": "10.4.15",
    "commitizen": "4.3.0",
    "cz-git": "1.7.1",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-mdx": "2.2.0",
    "eslint-plugin-prettier": "5.0.0",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "next-sitemap": "4.2.3",
    "pg": "8.11.3",
    "postcss": "8.4.29",
    "prettier": "3.0.3",
    "rehype-raw": "6.1.1",
    "remark-shiki-twoslash": "3.1.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "packageManager": "pnpm@8.6.12"
}
