{
  "name": "sidetrack-website",
  "version": "0.0.1",
  "description": "Sidetrack website",
  "homepage": "https://sidetrack.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidetracklabs/website.git"
  },
  "scripts": {
    "build": "next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "13.4.10",
    "nextra": "2.10.0",
    "nextra-theme-docs": "2.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.4",
    "sidetrack": "^0.0.8"
  },
  "devDependencies": {
    "@effect/io": "^0.37.1",
    "@mdx-js/mdx": "2.3.0",
    "@sidetrack/client-prisma": "^0.0.8",
    "@types/node": "20.4.5",
    "@types/pg": "^8.10.2",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "gh-pages": "5.0.0",
    "pg": "^8.11.2",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "rehype-raw": "^6.1.1",
    "remark-shiki-twoslash": "^3.1.3",
    "typescript": "5.1.6"
  },
  "packageManager": "pnpm@8.6.11"
}
